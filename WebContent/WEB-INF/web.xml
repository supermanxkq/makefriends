<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>makeFriends</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
      <servlet-name>addUser</servlet-name>
      <servlet-class>njxzc.action.addUser</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>addUser</servlet-name>
      <url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
      <servlet-name>userLogin</servlet-name>
      <servlet-class>njxzc.action.userLogin</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>userLogin</servlet-name>
      <url-pattern>/userLogin</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
      <servlet-name>relation</servlet-name>
      <servlet-class>njxzc.action.RelationShip</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>relation</servlet-name>
      <url-pattern>/relation</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>modify</servlet-name>
      <servlet-class>njxzc.action.modify</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>modify</servlet-name>
      <url-pattern>/modify</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>checkCode</servlet-name>
      <servlet-class>njxzc.utils.ValidateCode</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>checkCode</servlet-name>
      <url-pattern>/ValidateCode</url-pattern>
  </servlet-mapping>
  
</web-app>